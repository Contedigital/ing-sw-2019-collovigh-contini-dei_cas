<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.stage.Screen?>
<StackPane fx:id="stackPane" stylesheets="Murcielago.css" prefHeight="${screen.visualBounds.height}" prefWidth="${screen.visualBounds.width}" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.view.GUI.GuiEndController">
    <fx:define>
        <Screen fx:factory="getPrimary" fx:id="screen" />
    </fx:define>
    <children>
        <VBox fx:id="vBox" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label text="" />
                <Label text="FINE DEL GIOCO" />
                <Label text="" />
                <Separator prefWidth="200.0" />
                <Label text="" />
                <Label text="Punteggi : " />
                <Label text="" />
                <ListView fx:id="pointPlayersListView" layoutX="14.0" layoutY="21.0" prefHeight="145.0" prefWidth="100.0" >
                    <padding>
                        <Insets left="10.0" right="10.0"  />
                    </padding>
                </ListView>
            </children>
            <Separator layoutX="10.0" layoutY="125.0" prefWidth="200.0">
                <padding>
                    <Insets bottom="10.0" top="10.0" />
                </padding>
            </Separator>


            <Button onAction="#close" text="Chiudi" />
        </VBox>
    </children>
    <padding>
        <Insets left="100.0" right="100.0" top="150.0" bottom="50.0" />
    </padding>
</StackPane>
