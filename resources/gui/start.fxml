<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane fx:id="stackPane" prefHeight="400.0" prefWidth="600.0" stylesheets="/gui/Murcielago.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.view.GUI.GuiStartController">
<children>
    <VBox fx:id="vBox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
        <children>


            <Button mnemonicParsing="false" text="Login" onAction="#openLoginScene"/>
            <Separator prefWidth="200.0">
                <padding>
                    <Insets bottom="50.0" top="50.0" />
                </padding>
            </Separator>
            <Button mnemonicParsing="false" text="Riconnetti" onAction="#openReconnectScene"/>


        </children>
    </VBox>
</children>
<padding>
    <Insets bottom="50.0" left="100.0" right="100.0" top="150.0" />
</padding>
</StackPane>
